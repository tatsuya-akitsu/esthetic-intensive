{
  "name": "node-sass-magic-importer",
  "description": "Custom node-sass importer for selector specific imports, node importing, module importing, globbing support and importing files only once",
  "keywords": [
    "glob-importer",
    "importer",
    "module-importer",
    "node-sass",
    "package-importer",
    "sass",
    "selector-importer"
  ],
  "version": "5.0.3",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/node-sass-magic-importer/tree/master/packages/node-sass-magic-importer",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maoberlehner/node-sass-magic-importer"
  },
  "bugs": {
    "url": "https://github.com/maoberlehner/node-sass-magic-importer/issues"
  },
  "dependencies": {
    "css-node-extract": "^2.1.3",
    "css-selector-extract": "^3.3.6",
    "findup-sync": "^2.0.0",
    "glob": "^7.1.2",
    "object-hash": "^1.2.0",
    "postcss-scss": "^1.0.2",
    "resolve": "^1.5.0"
  },
  "devDependencies": {
    "@types/glob": "^5.0.34",
    "@types/node": "^8.5.1",
    "@types/resolve": "0.0.4",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.2"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.11.1",
    "npm": ">=3.0.0"
  }
}
